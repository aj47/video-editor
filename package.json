{
  "name": "gifizer",
  "productName": "Gifizer",
  "version": "0.2.3",
  "private": true,
  "author": "kyushun <kyushun12@gmail.com> (https://github.com/kyushun/)",
  "homepage": "https://github.com/kyushun/gifizer",
  "scripts": {
    "start": "vue-cli-service electron:serve",
    "build": "rimraf dist && vue-cli-service electron:build",
